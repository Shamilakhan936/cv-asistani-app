{
  "version": 2,
  "cleanUrls": true,
  "public": false,
  "buildCommand": "npm run build || exit 0",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "package.json", 
      "use": "@vercel/next", 
      "config": {
        "skipBuildSteps": ["typecheck"],
        "skipMiddlewareUrlNormalize": true
      }
    }
  ],
  "build": {
    "env": {
      "NEXT_SKIP_TYPECHECKING": "1",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
} 